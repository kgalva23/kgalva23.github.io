{
  "name": "sr-timeline-react-js",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:tw:watch\" \"vite\"",
    "build": "npm run tw:build && vite build",
    "preview": "vite preview",
    "tw:build": "tailwindcss -i ./src/index.css -o ./src/tailwind.css",
    "tw:watch": "tailwindcss -i ./src/index.css -o ./src/tailwind.css --watch"
  },
  "dependencies": {
    "framer-motion": "^11.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.15",
    "tailwindcss": "^4.1.15",
    "concurrently": "^9.0.0",
    "vite": "^5.4.10"
  }
}